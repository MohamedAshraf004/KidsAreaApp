@model Reservation

@{
    Layout = null;
}

<div class="row mt-3">
    <div class="col-md-12">
        <div class="col-md-3"></div>
        <div class="col-md-9">
            <img src="@String.Format("data:image/png;base64,{0}",
                                Convert.ToBase64String(Model.Receipt.BarCode))" height="300" width="300" />
        </div>
    </div>
</div>

@Model.StartReservationTme

<br />

@if (Model.Cost != 0)
    @Model.Cost

<hr />
<br />


